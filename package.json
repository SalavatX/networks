{
  "name": "social-network",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://SalavatX.github.io/social-net",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "init-db": "mysql -u root < server/database.sql",
    "migrate": "node server/migrate.cjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@types/date-fns": "^2.5.3",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "firebase": "^11.4.0",
    "firebase-admin": "^11.11.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "express": "^4.21.2",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "multer": "^1.4.5-lts.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
